<h1>Listing visit_procedures</h1>

<table>
  <thead>
    <tr>
      <th>Visit</th>
      <th>Patient</th>
      <th>Procedure</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visit_procedures.each do |visit_procedure| %>
      <tr>
        <td><%= visit_procedure.visit_id %></td>
        <td><%= visit_procedure.patient_id %></td>
        <td><%= visit_procedure.procedure_id %></td>
        <td><%= link_to 'Show', visit_procedure %></td>
        <td><%= link_to 'Edit', edit_visit_procedure_path(visit_procedure) %></td>
        <td><%= link_to 'Destroy', visit_procedure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visit procedure', new_visit_procedure_path %>
