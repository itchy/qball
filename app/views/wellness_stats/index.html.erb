<h1>Listing wellness_stats</h1>

<table>
  <thead>
    <tr>
      <th>String</th>
      <th>Uuid</th>
      <th>Json</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wellness_stats.each do |wellness_stat| %>
      <tr>
        <td><%= wellness_stat.string %></td>
        <td><%= wellness_stat.uuid %></td>
        <td><%= wellness_stat.json %></td>
        <td><%= link_to 'Show', wellness_stat %></td>
        <td><%= link_to 'Edit', edit_wellness_stat_path(wellness_stat) %></td>
        <td><%= link_to 'Destroy', wellness_stat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wellness stat', new_wellness_stat_path %>
