<h1>Listing visit_diagnoses</h1>

<table>
  <thead>
    <tr>
      <th>Visit</th>
      <th>Patient</th>
      <th>Diagnosis</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visit_diagnoses.each do |visit_diagnosis| %>
      <tr>
        <td><%= visit_diagnosis.visit_id %></td>
        <td><%= visit_diagnosis.patient_id %></td>
        <td><%= visit_diagnosis.diagnosis_id %></td>
        <td><%= link_to 'Show', visit_diagnosis %></td>
        <td><%= link_to 'Edit', edit_visit_diagnosis_path(visit_diagnosis) %></td>
        <td><%= link_to 'Destroy', visit_diagnosis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visit diagnosis', new_visit_diagnosis_path %>
