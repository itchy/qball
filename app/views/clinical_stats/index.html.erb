<h1>Listing clinical_stats</h1>

<table>
  <thead>
    <tr>
      <th>String</th>
      <th>Uuid</th>
      <th>Json</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clinical_stats.each do |clinical_stat| %>
      <tr>
        <td><%= clinical_stat.string %></td>
        <td><%= clinical_stat.uuid %></td>
        <td><%= clinical_stat.json %></td>
        <td><%= link_to 'Show', clinical_stat %></td>
        <td><%= link_to 'Edit', edit_clinical_stat_path(clinical_stat) %></td>
        <td><%= link_to 'Destroy', clinical_stat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clinical stat', new_clinical_stat_path %>
