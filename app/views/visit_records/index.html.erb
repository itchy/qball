<h1>Listing visit_records</h1>

<table>
  <thead>
    <tr>
      <th>Visit</th>
      <th>Patient</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visit_records.each do |visit_record| %>
      <tr>
        <td><%= visit_record.visit_id %></td>
        <td><%= visit_record.patient_id %></td>
        <td><%= visit_record.data %></td>
        <td><%= link_to 'Show', visit_record %></td>
        <td><%= link_to 'Edit', edit_visit_record_path(visit_record) %></td>
        <td><%= link_to 'Destroy', visit_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visit record', new_visit_record_path %>
